{
    "name": "tnmdev/auth-service",
    "description": "Service integration for Auth Server",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "TNM Developers",
            "email": "developers@tnm.co.mw"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "doctrine/dbal": "^2.9",
        "laravel/passport": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "TNM\\AuthService\\": "src/"
        }
    },
    "autoload-dev" : {
        "psr-4": {
            "TNM\\AuthService\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TNM\\AuthService\\AuthServiceProvider"
            ]
        }
    }
}
